
create database mobileapi;

use mobileapi;

create table shopping (
  id int primary key auto_increment not null,
  nome varchar(255),
  rating varchar(255),
  endereco varchar(255)
);

create table user (
  id int primary key auto_increment not null,
  login varchar(255),
  senha varchar(255)
);

